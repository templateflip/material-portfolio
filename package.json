{
  "name": "material-portfolio",
  "version": "1.0.1",
  "description": "A Material design portfolio template built with Material Design Light framework by TemplateFlip.com",
  "scripts": {
    "clean": "mmpilot clean",
    "build": "mmpilot build",
    "serve": "mmpilot serve",
    "dev-build": "mmpilot build -d",
    "dev-watch": "mmpilot watch -d",
    "dev": "npm-run-all clean dev-build --parallel dev-watch serve",
    "prod": "npm-run-all clean build serve",
    "deploy": "npm-run-all clean build && mmpilot deploy",
    "dist": "npm run dev-build && node dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/templateflip/material-portfolio.git"
  },
  "author": "TemplateFlip.com",
  "license": "Creative Commons Attribution 3.0 Unported License",
  "bugs": {
    "url": "https://github.com/templateflip/material-portfolio/issues"
  },
  "homepage": "https://github.com/templateflip/material-portfolio#readme",
  "devDependencies": {
    "mmpilot": "^0.6.3",
    "npm-run-all": "^4.0.1",
    "shelljs": "^0.7.6"
  },
  "dependencies": {}
}
